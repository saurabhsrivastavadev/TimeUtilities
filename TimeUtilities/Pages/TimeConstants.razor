@page "/constants"

@using DotNetUtils.DateAndTime

<p>
    <h1 class="badge">UTC Epoch</h1>
    <h5>@string.Format("{0:yyyy MMM dd, hh:mm:ss tt}", DateTime.UnixEpoch)</h5>
</p>

<p>
    <h1 class="badge">GPS Epoch</h1>
    <h5>@string.Format("{0:yyyy MMM dd, hh:mm:ss tt}", DateTimeConstants.GpsEpoch)</h5>
</p>
